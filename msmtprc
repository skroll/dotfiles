account gmail
host smtp.gmail.com
port 587
auth on
tls on
tls_trust_file ~/.local/etc/ssl/certs/ca-certificates.pem
user skroll@gmail.com
passwordeval echo "import keyring\nprint keyring.get_password('imap.gmail.com', 'skroll@gmail.com')" | python
auto_from on

account kevadiya
host smtp.gmail.com
port 587
auth on
tls on
tls_trust_file ~/.local/etc/ssl/certs/ca-certificates.pem
passwordeval echo "import keyring\nprint keyring.get_password('imap.gmail.com', 'skroll@kevadiya.com')" | python
auto_from on
